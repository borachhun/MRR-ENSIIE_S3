# BATS predicted CHL 2-18
log_CHL_predict <- function(test_data, model_list) {
# 219 = 73 x 3 (3 years)
res <- rep(0, times=73)
# 7:23 = CHL 2-18
test_data <- test_data[, -c(7:23)]
for (i in 1:17) {
res <- cbind(res, predict(model_list[[i]], as.matrix(test_data)))
#res <- cbind(res, predict(model_list[[i]], test_data))
}
return(res[,-1])
}
